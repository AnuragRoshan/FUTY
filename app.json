    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "https://images.unsplash.com/photo-1517747614396-d21a78b850e8?q=80&w=1854&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D",
